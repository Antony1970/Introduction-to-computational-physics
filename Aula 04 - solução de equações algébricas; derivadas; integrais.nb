Cell[BoxData[
 RowBox[{
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"x", "+", "y"}], "\[Equal]", "z"}], ",", 
      RowBox[{
       RowBox[{"10", "-", 
